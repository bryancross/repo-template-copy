{
  "name": "repo-template",
  "version": "1.0.0",
  "description": "Create new GitHub repositories based on templates",
  "main": "repo-template.js",
  "scripts": {
    "test": "xo && server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bryancross/repo-template.git"
  },
  "keywords": [
    "repository",
    "GitHub",
    "template"
  ],
  "author": "bryancross@github.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bryancross/repo-template/issues"
  },
  "homepage": "https://github.com/bryancross/repo-template#readme",
  "dependencies": {
    "bluebird": "^3.4.7",
    "cli-spinner": "^0.2.5",
    "date-fns": "^1.24.0",
    "deep-diff": "latest",
    "github": "^7.3.2",
    "github-username-regex": "^1.0.0",
    "hashmap": "^2.1.0",
    "httpdispatcher": "^2.0.1",
    "inquirer": "^3.1.1",
    "js-base64": "^2.1.9",
    "jshint": "^2.11.0",
    "json-path-processor": "^0.2.3",
    "mkdirp": "^0.5.1",
    "renderjson": "^1.3.1",
    "request": "^2.79.0",
    "request-json": "0.6.2",
    "rimraf": "^2.5.4",
    "url-parse": "^1.5.8"
  },
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "engines": {
    "node": "6.11.1"
  },
  "xo": {
    "overrides": [
      {
        "files": "worker.js",
        "rules": {
          "camelcase": [
            "error",
            {
              "properties": "never"
            }
          ]
        }
      }
    ]
  }
}
